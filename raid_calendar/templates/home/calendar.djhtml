{% extends "home/wrap.djhtml" %}
{% block content %}
<h1>Drama Llama Raid Calendar</h1>
<h2>{{date}}</h2>
<div class="cal-wrap">
  {% for date in days %}
  <span class="cal-cell">
	{{date.day|default:""}}<br/>
	{% if date.raids %}
	{% for raid in date.raids %}<a href="{{raid.url}}">{{ raid.title }}</a>{% endfor %}<br/>
	{% endif %}
  </span>
  {% endfor %}
{% endblock %}

